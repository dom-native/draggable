{
  "name": "@dom-native/draggable",
  "type": "module",
  "version": "0.3.0-alpha.1",
  "description": "Minimalist and expressive API for dragging, positioning, and 'FLIP - capture/invert/play'",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/dom-native/draggable.git"
  },
  "keywords": [
    "native",
    "Draggable",
    "web-component",
    "ui components",
    "html5",
    "DOM",
    "Drag and Drop"
  ],
  "devDependencies": {
    "@dom-native/demo-infra": "^0.0.0-internal-5",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash.debounce": "^4.0.9",
    "autoprefixer": "^10.4.16",
    "chokidar": "^3.5.3",
    "cloud-bucket": "^0.4.2",
    "cmdrouter": "^0.3.1",
    "fs-extra-plus": "^0.6.0",
    "js-yaml": "^4.1.0",
    "lodash.debounce": "^4.0.8",
    "p-spawn": "^0.5.5",
    "pcss-cli": "^0.2.9",
    "rollup": "^4.9.4",
    "dom-native": "^0.12.0-alpha.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "utils-min": "^0.2.3"
  },
  "files": [
    "src/",
    "dist/"
  ],
  "scripts": {
    "prepack": "npm run build",
    "build": "TS_NODE_PROJECT='scripts/tsconfig.json' node --loader ts-node/esm ./scripts/cmds.ts build",
    "build-demo-js": "./node_modules/.bin/rollup -c",
    "pcss": "pcss",
    "rollup": "rollup -c",
    "build-demo-css": "./node_modules/.bin/postcss -o demo/dist/demo-bundle.css  demo/pcss/main.pcss",
    "watch": "TS_NODE_PROJECT='scripts/tsconfig.json' node --loader ts-node/esm ./scripts/cmds.ts watch"
  },
  "engines": {
    "node": ">=8.2.0"
  },
